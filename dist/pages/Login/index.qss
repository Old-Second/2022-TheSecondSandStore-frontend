    width: 400rpx;
    height: 75rpx;
    margin-left: 175rpx;
    font-size: 30rpx;
    padding-left: 40rpx;
    border-radius: 40rpx;
    border: 1rpx solid rgb(159, 158, 158);
    background-color: white;
}


.passWord .input1{
    margin-bottom: 55rpx;
}


.passWord .input2{
    margin-bottom: 70rpx;
}


.btn1{
    width: 175rpx;
    height: 70rpx;
    color:#6c6c6c;
    font-size: 40rpx;
