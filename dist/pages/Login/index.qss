.Login{
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    width: 750rpx;
    height: 1205rpx;
    -webkit-flex-direction:column;
        -ms-flex-direction:column;
            flex-direction:column;
    -webkit-justify-content: center;
        -ms-flex-pack: center;
            justify-content: center;
 }


.Login Image{
  width : 100%;
  height : 100%; 
  position: fixed;
  z-index: -1; 
  background-size: 100% 100%;
  opacity: 0.3;
  
}


.header{
    font-size: 68rpx;
    text-align: center;
    margin-bottom: 200rpx;
}


.header Text{
    color: #605757;
    text-shadow:3rpx 4rpx 5rpx #BFBEBD;
}


